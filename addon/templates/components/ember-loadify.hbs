{{yield (hash
  records=records
  isEmpty=isEmpty
  isLastPage=isLastPage
  canLoadMore=canLoadMore
  isLoading=isLoading
  isResetting=isResetting
  nextPage=(action "nextPage")
  reset=(perform resetRecords))}}
{{#if showPagination}}
  {{#if canLoadMore}}
    {{ember-loadify/pagination infinite=infinite text=nextPageText onNextPage=(action "nextPage")}}
  {{/if}}
{{/if}}
